{
    "name": "feldsam-inc/paybysquare-php",
    "description": "PHP library for generating PAY by square payment QR codes",
    "keywords": [
        "paybysquare",
        "qr-code",
        "payment",
        "qr-payment",
        "slovakia",
        "czech-republic"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "FELDSAM s.r.o.",
            "email": "support@feldhost.cz"
        }
    ],
    "require": {
        "php": ">=7.4",
        "endroid/qr-code": "^4.0 || ^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "PayBySquare\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PayBySquare\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
